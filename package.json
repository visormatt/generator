{
  "name": "generator",
  "version": "1.0.0",
  "author": {
    "name": "Matthew D. Scholta",
    "email": "matthew.scholta@gmail.com",
    "url": "https://www.mattscholta.com"
  },
  "bin": {
    "generate": "./dist/index.js",
    "generate:setup": "./dist/setup.js"
  },
  "description": "A simple and customizable Generator. \nWritten in ES6 (babel) this generator allows the end user to configure the custom templates... \nSpend less time writing boiler-plate code and more time writing the fun stuff.",
  "dependencies": {
    "chalk": "^2.4.1",
    "ejs": "^2.6.2",
    "find-up": "^4.1.0",
    "inquirer": "^6.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.4.4",
    "@types/ejs": "^2.6.0",
    "@types/find-up": "^2.1.1",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/rewire": "^2.5.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "codecov": "^3.5.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.13",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typedoc": "^0.14.0",
    "typescript": "^3.5.2"
  },
  "files": [
    "generators",
    "templates"
  ],
  "homepage": "https://github.com/visormatt/generator",
  "main": "dist/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --config .eslintrc.js"
    ],
    "*.{ts,tsx}": [
      "tslint --project tsconfig.json --config tslint.json'"
    ]
  },
  "repository": "visormatt/generator",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:dev": "npm run build:js -- --source-maps inline --watch",
    "build:docs": "typedoc --exclude '**/*.test.ts,src/examples/*' --out typedocs/ src/",
    "build:js": "babel src --ignore '**/__test__/,**/__mocks__/' --out-dir ./dist --extensions \".js,.ts,.tsx\" --source-maps inline ",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "npm run lint:ts",
    "lint:js": "eslint ./src",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "setup": "node dist/setup.js",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:ci": "jest --coverage && codecov --token=31f58f79-fa54-48ae-bb95-9112cfded2d9",
    "test:coverage": "jest --coverage --watch",
    "test:logs": "jest --coverage --runInBand --watch",
    "test:update": "jest --updateSnapshot --watchAll --watch",
    "test:watch": "jest --onlyChanged --watch",
    "typescript:check": "tsc --noEmit",
    "typescript:check:watch": "npm run typescript:check -- --watch",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A"
  }
}
